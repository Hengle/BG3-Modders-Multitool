<Window x:Class="bg3_modders_multitool.Views.Update"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:p = "clr-namespace:bg3_modders_multitool.Properties"
        xmlns:mdxam="clr-namespace:MdXaml;assembly=MdXaml"
        ResizeMode="NoResize"
        mc:Ignorable="d"
        Title="{x:Static p:Resources.NewUpdatesAvailable}" Height="500" Width="600">
    <Grid>
        <mdxam:MarkdownScrollViewer xml:space="preserve" x:Name="markdownViewer"/>
    </Grid>
</Window>
